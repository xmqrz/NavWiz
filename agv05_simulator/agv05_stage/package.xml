<?xml version="1.0"?>
<package format="3">
  <name>agv05_stage</name>
  <version>2.0.0</version>
  <description>
    The agv05_stage package provides a simulator mobile robot using the Stage library.
  </description>
  <author email="patrick@dfautomation.com">Patrick Chin</author>
  <maintainer email="patrick@dfautomation.com">Patrick Chin</maintainer>
  <license file="COPYRIGHT">Copyright (C) 2017 DF Automation and Robotics Sdn. Bhd.</license>
  <url type="repository">https://gitlab.com/dfautomation/product/navwiz/agv05_simulator</url>
  <url type="bugtracker">https://gitlab.com/dfautomation/product/navwiz/agv05_simulator/issues</url>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>geometry_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>roscpp</depend>
  <depend>rosgraph_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>stage</depend>
  <depend>tf2_ros</depend>

  <exec_depend>agv05_capabilities</exec_depend>

  <test_depend>roslaunch</test_depend>
  <test_depend>roslint</test_depend>

  <export>
    <capability_provider>
      providers/stage_node.yaml
    </capability_provider>
  </export>
</package>
