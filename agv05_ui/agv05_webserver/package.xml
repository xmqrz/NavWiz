<?xml version="1.0"?>
<package format="3">
  <name>agv05_webserver</name>
  <version>3.2.0</version>
  <description>AGV05 Web Server</description>

  <author email="patrick@dfautomation.com">Patrick Chin</author>
  <maintainer email="patrick@dfautomation.com">Patrick Chin</maintainer>
  <license file="COPYRIGHT">Copyright (C) 2015 DF Automation and Robotics Sdn. Bhd.</license>
  <url type="repository">http://gitlab.com/dfautomation/product/navwiz/agv05_ui</url>
  <url type="bugtracker">http://gitlab.com/dfautomation/product/navwiz/agv05_ui/issues</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>libmysqlclient-dev</build_depend>
  <build_depend>pip_install</build_depend>
  <build_depend version_eq="2.7.17-1~18.24ubuntu1.11+df" condition="$ROS_PYTHON_VERSION == 2">python-dev</build_depend>
  <build_depend version_eq="3.8.10-0ubuntu1~20.24.13+df" condition="$ROS_PYTHON_VERSION == 3">python3-dev</build_depend>

  <exec_depend>agv05_executive_msgs</exec_depend>
  <exec_depend>agv05_msgs</exec_depend>
  <exec_depend>agv05_webapp</exec_depend>
  <exec_depend>capabilities</exec_depend>
  <exec_depend>chrony</exec_depend>
  <exec_depend>diagnostic_msgs</exec_depend>
  <exec_depend>dynamic_reconfigure</exec_depend>
  <exec_depend>dynamic_reconfigure_mysql_storage</exec_depend>
  <exec_depend>dynamic_reconfigure_sqlite_storage</exec_depend>
  <exec_depend>ifplugd</exec_depend>
  <exec_depend>image_geometry</exec_depend>
  <exec_depend>linux_wifi</exec_depend>
  <exec_depend>mysql-server</exec_depend>
  <exec_depend version_eq="2.7.17-1~18.24ubuntu1.11+df" condition="$ROS_PYTHON_VERSION == 2">python</exec_depend>
  <exec_depend version_eq="3.8.10-0ubuntu1~20.24.13+df" condition="$ROS_PYTHON_VERSION == 3">python3</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 2">python-imaging</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 2">python-opencv</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 2">python-rospkg</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 2">python-yaml</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 3">python3-pil</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 3">python3-opencv</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 3">python3-rospkg</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 3">python3-yaml</exec_depend>
  <exec_depend>redis-server</exec_depend>
  <exec_depend>rospy</exec_depend>
  <exec_depend>tf</exec_depend>
  <exec_depend>usbmount</exec_depend>

  <test_depend>roslaunch</test_depend>
  <test_depend>roslint</test_depend>
  <test_depend>rostest</test_depend>
</package>
