<?xml version="1.0"?>
<package format="3">
  <name>agv05_navx</name>
  <version>3.2.0</version>
  <description>The agv05_navx package</description>
  <author email="phtan@dfautomation.com">Tan Ping Hua</author>
  <maintainer email="phtan@dfautomation.com">Tan Ping Hua</maintainer>
  <license file="COPYRIGHT">Copyright (C) 2015 DF Automation and Robotics Sdn. Bhd.</license>
  <url type="repository">https://gitlab.com/dfautomation/product/navwiz/agv05</url>
  <url type="bugtracker">https://gitlab.com/dfautomation/product/navwiz/agv05/issues</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>agv05_lidar</build_depend>
  <build_depend>boost</build_depend>
  <build_depend>cgal</build_depend>
  <build_depend>libjsoncpp-dev</build_depend>
  <build_depend>tf2_geometry_msgs</build_depend>

  <depend>actionlib</depend>
  <depend>agv05_msgs</depend>
  <depend>agv05_nav</depend>
  <depend>costmap_2d</depend>
  <depend>diagnostic_updater</depend>
  <depend>dynamic_reconfigure</depend>
  <depend>geometry_msgs</depend>
  <depend>global_planner</depend>
  <depend>nav_msgs</depend>
  <depend>roscpp</depend>
  <depend>std_srvs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>

  <exec_depend>libjsoncpp</exec_depend>
  <exec_depend>rospy</exec_depend>

  <test_depend>roslint</test_depend>

  <export>
    <costmap_2d plugin="${prefix}/costmap_plugins.xml" />
  </export>

</package>
