<launch>
  <rosparam param="primary_obstacle_scan_topics">
    scan: Laser Primary Front
    scan2: Laser Secondary Back
  </rosparam>
  <param name="secondary_obstacle_scan_topics" value="scan2 scan3 scan4" />
  <rosparam param="fourth_obstacle_scan_topics">
    hokuyo/scan: Laser Primary Front
    hokuyo/scan2: Laser Secondary Back
    sick/scan3: Rear Hidden Lidar
    scan4: Main Top Lidar
  </rosparam>
  
  <test test-name="test_file_cpp" pkg="agv05_lidar" type="agv05_lidar-cpp_test">
    <remap from="~primary_obstacle_scan_topics" to="/primary_obstacle_scan_topics"/>
    <remap from="~secondary_obstacle_scan_topics" to="/secondary_obstacle_scan_topics"/>
    <remap from="~tertiary_obstacle_scan_topics" to="/tertiary_obstacle_scan_topics"/>
    <remap from="~fourth_obstacle_scan_topics" to="/fourth_obstacle_scan_topics"/>
  </test>
</launch>
